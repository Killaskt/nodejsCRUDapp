{
  "name": "nodejscrud",
  "version": "1.0.0",
  "description": "Shopping list made with MERN stack, items will be checked on amazon and then suggested",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "clientAlternative": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Shubham Tiwari",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.3",
    "express-handlebars": "^4.0.4",
    "mongoose": "^5.9.14",
    "puppeteer": "^3.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
